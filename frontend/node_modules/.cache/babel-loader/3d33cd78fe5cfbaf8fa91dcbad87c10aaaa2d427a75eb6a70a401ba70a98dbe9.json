{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport NavigationBar from './NavigationBar.vue';\nexport default {\n  components: {\n    NavigationBar\n  },\n  name: 'LoginPage',\n  data() {\n    return {\n      loginForm: {\n        loginName: '',\n        password: ''\n      },\n      selectedMethod: '' // 新增变量用于绑定下拉框的选中项\n    };\n  },\n  methods: {\n    login() {\n      console.log('submit!', this.loginForm);\n      this.$message({\n        message: '识别成功',\n        type: 'success',\n        duration: 1500 // 设置持续时间为1.5秒\n      });\n      this.$router.push({\n        path: '/Home'\n      });\n    },\n    toRegister() {\n      this.$router.push({\n        path: '/Register'\n      });\n    },\n    handleSelect(key) {\n      switch (key) {\n        case '1':\n          this.$router.push({\n            path: '/Login'\n          });\n          break;\n        case '2':\n          this.$router.push({\n            path: '/About'\n          });\n          break;\n      }\n    },\n    beforeUpload(file) {\n      // 在这里添加文件验证逻辑，比如检查文件类型、大小等\n      // 返回true则允许上传，返回false则阻止上传\n      // 这里仅为示例，未提供具体验证逻辑\n      return true;\n    }\n  }\n};","map":{"version":3,"names":["NavigationBar","components","name","data","loginForm","loginName","password","selectedMethod","methods","login","console","log","$message","message","type","duration","$router","push","path","toRegister","handleSelect","key","beforeUpload","file"],"sources":["E:\\VsCode\\Vue3\\vue0323\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n    <div id=\"app\">\r\n        <!-- 导航栏 -->\r\n        <NavigationBar />\r\n        <div id=\"bg-image\"></div>\r\n\r\n        <body id=\"poster\">\r\n            <el-form class=\"login-container\" label-position=\"left\" label-width=\"0px\">\r\n                <h3 class=\"login_title\"> 情绪识别系统\r\n                    <!-- <el-button @click=\"toRegister()\">注册</el-button> -->\r\n                </h3>\r\n\r\n                <el-form-item label=\"\">\r\n                    <el-upload action=\"\" :auto-upload=\"false\" :before-upload=\"beforeUpload\"\r\n                        accept=\".txt, .csv, application/vnd.ms-excel, application/vnd.openxmlformats-officedocument.spreadsheetml.sheet\"\r\n                        style=\"display:inline-block;width:100%\" placeholder=\"点击上传数据集\">\r\n                        <el-button slot=\"trigger\" size=\"small\" type=\"primary\">上传文件</el-button>\r\n                    </el-upload>\r\n                </el-form-item>\r\n                <el-form-item label=\"\">\r\n                    <el-select v-model=\"selectedMethod\" placeholder=\"   选择方法\" style=\"width: 350px;\">\r\n                        <el-option label=\"CNN\" value=\"CNN\"></el-option>\r\n                        <el-option label=\"SVM\" value=\"SVM\"></el-option>\r\n                    </el-select>\r\n                </el-form-item>\r\n\r\n                <el-form-item>\r\n                    <el-button type=\"primary\" style=\"width:100%;background: #505458;border: none;\"\r\n                        @click=\"login()\">开始识别</el-button>\r\n                </el-form-item>\r\n            </el-form>\r\n        </body>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport NavigationBar from './NavigationBar.vue';\r\nexport default {\r\n    components: {\r\n        NavigationBar\r\n    },\r\n    name: 'LoginPage',\r\n    data() {\r\n        return {\r\n            loginForm: {\r\n                loginName: '',\r\n                password: ''\r\n            },\r\n            selectedMethod: '', // 新增变量用于绑定下拉框的选中项\r\n        }\r\n    },\r\n    methods: {\r\n        login() {\r\n            console.log('submit!', this.loginForm);\r\n            this.$message({\r\n                message: '识别成功',\r\n                type: 'success',\r\n                duration: 1500 // 设置持续时间为1.5秒\r\n            });\r\n            this.$router.push({\r\n                path: '/Home'\r\n            })\r\n        },\r\n        toRegister() {\r\n            this.$router.push({\r\n                path: '/Register'\r\n            })\r\n        },\r\n        handleSelect(key) {\r\n            switch (key) {\r\n                case '1':\r\n                    this.$router.push({ path: '/Login' });\r\n                    break;\r\n                case '2':\r\n                    this.$router.push({ path: '/About' });\r\n                    break;\r\n            }\r\n        },\r\n        beforeUpload(file) {\r\n            // 在这里添加文件验证逻辑，比如检查文件类型、大小等\r\n            // 返回true则允许上传，返回false则阻止上传\r\n            // 这里仅为示例，未提供具体验证逻辑\r\n            return true;\r\n        },\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n#app {\r\n    height: 100vh;\r\n    overflow: hidden;\r\n}\r\n\r\n#bg-image {\r\n    position: absolute;\r\n    top: 0;\r\n    left: 0;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: -1;\r\n    /* 确保背景图层位于最下层 */\r\n    background-image: url('@/assets/32.png');\r\n    background-position: center;\r\n    background-repeat: no-repeat;\r\n    background-size: cover;\r\n    opacity: 0.5;\r\n    /* 调整此值以改变背景图片的透明度 */\r\n}\r\n\r\n#poster {\r\n    background-position: center;\r\n    height: 100%;\r\n    width: 100%;\r\n    background-size: cover;\r\n    position: fixed;\r\n\r\n}\r\n\r\nbody {\r\n    margin: 0px;\r\n    padding: 0px;\r\n}\r\n\r\n.login-container {\r\n    border-radius: 15px;\r\n    background-clip: padding-box;\r\n    margin: 90px auto;\r\n    width: 350px;\r\n    padding: 35px 35px 15px 35px;\r\n    background: transparent;\r\n    border: 1px solid #eaeaea;\r\n    box-shadow: 0 0 25px #cac6c6;\r\n}\r\n\r\n.login_title {\r\n    margin: 0px auto 40px auto;\r\n    text-align: center;\r\n    color: #505458;\r\n}\r\n\r\n.el-menu {\r\n    display: flex;\r\n    justify-content: center;\r\n    margin-bottom: 20px;\r\n\r\n}\r\n\r\n.el-menu-item {\r\n    font-size: 14px;\r\n\r\n}\r\n\r\n.el-input__inner {\r\n    background-color: transparent !important;\r\n    /* 可以根据需要调整边框样式 */\r\n    border: 1px solid rgba(255, 255, 255, 0.5) !important;\r\n}\r\n\r\n.el-input.is-active .el-input__inner::placeholder,\r\n.el-input__inner::placeholder {\r\n    color: #4D4D4D !important;\r\n}\r\n\r\n.el-upload {\r\n    background-color: transparent;\r\n    border-radius: 100px;\r\n    display: flex;\r\n    align-items: center;\r\n    justify-content: center;\r\n}\r\n\r\n.el-upload:hover {\r\n    background-color: transparent;\r\n}\r\n\r\n.el-upload>.el-button {\r\n    background-color: transparent;\r\n    color: #4D4D4D;\r\n    width: 440%;\r\n}\r\n\r\n.el-upload__tip {\r\n    color: transparent;\r\n}\r\n\r\n.el-select .el-input__inner::placeholder {\r\n    text-align: center;\r\n}\r\n</style>"],"mappings":";AAoCA,OAAOA,aAAY,MAAO,qBAAqB;AAC/C,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,IAAI,EAAE,WAAW;EACjBC,IAAIA,CAAA,EAAG;IACH,OAAO;MACHC,SAAS,EAAE;QACPC,SAAS,EAAE,EAAE;QACbC,QAAQ,EAAE;MACd,CAAC;MACDC,cAAc,EAAE,EAAE,CAAE;IACxB;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,KAAKA,CAAA,EAAG;MACJC,OAAO,CAACC,GAAG,CAAC,SAAS,EAAE,IAAI,CAACP,SAAS,CAAC;MACtC,IAAI,CAACQ,QAAQ,CAAC;QACVC,OAAO,EAAE,MAAM;QACfC,IAAI,EAAE,SAAS;QACfC,QAAQ,EAAE,IAAG,CAAE;MACnB,CAAC,CAAC;MACF,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC;QACdC,IAAI,EAAE;MACV,CAAC;IACL,CAAC;IACDC,UAAUA,CAAA,EAAG;MACT,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QACdC,IAAI,EAAE;MACV,CAAC;IACL,CAAC;IACDE,YAAYA,CAACC,GAAG,EAAE;MACd,QAAQA,GAAG;QACP,KAAK,GAAG;UACJ,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC;YAAEC,IAAI,EAAE;UAAS,CAAC,CAAC;UACrC;QACJ,KAAK,GAAG;UACJ,IAAI,CAACF,OAAO,CAACC,IAAI,CAAC;YAAEC,IAAI,EAAE;UAAS,CAAC,CAAC;UACrC;MACR;IACJ,CAAC;IACDI,YAAYA,CAACC,IAAI,EAAE;MACf;MACA;MACA;MACA,OAAO,IAAI;IACf;EACJ;AACJ","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}