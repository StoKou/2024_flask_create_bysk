  return {\n      activeIndex,\n      onSelect\n    };\n  }\n};\n</script>\n\n<style scoped>\n.el-menu-demo {\n  background-color: #f0f0f0;\n}\n</style>"],"mappings":"AAqBA,SAASA,IAAI,EAAEC,QAAQ,EAAEC,OAAM,QAAS,cAAc;AACtD,SAASC,GAAE,QAAS,KAAK;AACzB,SAASC,QAAO,QAAS,YAAY;AAErC,eAAe;EACbC,UAAU,EAAE;IACV,CAACL,IAAI,CAACM,IAAI,GAAGN,IAAI;IACjB,CAACC,QAAQ,CAACK,IAAI,GAAGL,QAAQ;IACzB,CAACC,OAAO,CAACI,IAAI,GAAGJ;EAClB,CAAC;EACDK,KAAKA,CAAA,EAAG;IACN,MAAMC,KAAI,GAAIJ,QAAQ,CAAC,CAAC;IACxB,MAAMK,WAAU,GAAIN,GAAG,CAACK,KAAK,CAACE,IAAI,CAAC;IAEnC,MAAMC,QAAO,GAAKC,GAAG,IAAK;MACxBC,OAAO,CAACC,GAAG,CAAE,aAAYF,GAAI,EAAC,CAAC;IACjC,CAAC;IAED,OAAO;MACLH,WAAW;MACXE;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}