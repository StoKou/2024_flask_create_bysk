{"ast":null,"code":"import { ElNavbar, ElHeader, ElMenu, ElMenuItem } from 'element-plus';\nimport { ref } from 'vue'; // 引入ref\nexport default {\n  components: {\n    ElNavbar,\n    ElHeader,\n    ElMenu,\n    ElMenuItem\n  },\n  setup() {\n    const isCollapse = ref(false); // 假设有一个响应式的折叠状态变量\n\n    // 处理菜单项选择事件\n    const handleSelect = (index, indexPath) => {\n      console.log('Selected menu item index:', index);\n    };\n    return {\n      isCollapse,\n      handleSelect\n    };\n  }\n};","map":{"version":3,"names":["ElNavbar","ElHeader","ElMenu","ElMenuItem","ref","components","setup","isCollapse","handleSelect","index","indexPath","console","log"],"sources":["E:\\VsCode\\Vue3\\vue0323\\frontend\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <!-- 使用Element Plus的ElNavbar组件 -->\n    <el-navbar type=\"light\" :collapse=\"isCollapse\">\n      <el-container>\n        <el-header>\n          <el-menu mode=\"horizontal\" collapse-transition @select=\"handleSelect\">\n            <el-menu-item index=\"/\" exact>\n              <router-link to=\"/\">首页</router-link>\n            </el-menu-item>\n            <el-menu-item index=\"/about\">\n              <router-link to=\"/about\">关于我们</router-link>\n            </el-menu-item>\n            <el-menu-item index=\"/contact\">\n              <router-link to=\"/contact\">联系我们</router-link>\n            </el-menu-item>\n            <el-menu-item index=\"/expimage\">\n              <router-link to=\"/expimage\">实验图像（Excel图像测试阶段）</router-link>\n            </el-menu-item>\n            <el-menu-item index=\"/echartsdemo\">\n              <router-link to=\"/echartsdemo\">ECharts</router-link>\n            </el-menu-item>\n            <el-menu-item index=\"/predictpage\">\n              <router-link to=\"/predictpage\">predict</router-link>\n            </el-menu-item>\n          </el-menu>\n        </el-header>\n      </el-container>\n      \n      <!-- 路由视图，用于呈现匹配到的不同组件 -->\n      <router-view />\n    </el-navbar>\n  </div>\n</template>\n\n<script>\nimport { ElNavbar, ElHeader, ElMenu, ElMenuItem } from 'element-plus';\nimport { ref } from 'vue'; // 引入ref\nexport default {\n  components: {\n    ElNavbar,\n    ElHeader,\n    ElMenu,\n    ElMenuItem,\n  },\n  setup() {\n    const isCollapse = ref(false); // 假设有一个响应式的折叠状态变量\n\n    // 处理菜单项选择事件\n    const handleSelect = (index, indexPath) => {\n      console.log('Selected menu item index:', index);\n    };\n\n    return {\n      isCollapse,\n      handleSelect,\n    };\n  },\n};\n</script>\n\n<style scoped>\n/* 样式代码 */\n/* 可以在此添加 Element Plus 组件自定义样式 */\n</style>"],"mappings":"AAoCA,SAASA,QAAQ,EAAEC,QAAQ,EAAEC,MAAM,EAAEC,UAAS,QAAS,cAAc;AACrE,SAASC,GAAE,QAAS,KAAK,EAAE;AAC3B,eAAe;EACbC,UAAU,EAAE;IACVL,QAAQ;IACRC,QAAQ;IACRC,MAAM;IACNC;EACF,CAAC;EACDG,KAAKA,CAAA,EAAG;IACN,MAAMC,UAAS,GAAIH,GAAG,CAAC,KAAK,CAAC,EAAE;;IAE/B;IACA,MAAMI,YAAW,GAAIA,CAACC,KAAK,EAAEC,SAAS,KAAK;MACzCC,OAAO,CAACC,GAAG,CAAC,2BAA2B,EAAEH,KAAK,CAAC;IACjD,CAAC;IAED,OAAO;MACLF,UAAU;MACVC;IACF,CAAC;EACH;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}